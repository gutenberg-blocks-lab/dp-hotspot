{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/hotspot",
	"version": "0.1.0",
	"title": "Hotspot",
	"category": "widgets",
	"icon": "marker",
	"description": "Placehorder for images that can you ",
	"example": {},
	"supports": {
		"html": true,
		"align": [
			"wide",
			"full"
		]
	},
	"attributes": {
		"hotspotNumbers": {
			"type": "array",
			"default": []
		},
		"startNumber": {
			"type": "number",
			"default": 1
		},
		"hotspotBackgroundColor": {
			"type": "string",
			"default": "#D8613C"
		},
		"hotspotTextColor": {
			"type": "string",
			"default": "#FFF555"
		}
	},
	"textdomain": "hotspot",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}